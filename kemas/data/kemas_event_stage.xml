<?xml version="1.0" encoding="utf-8"?>
<openerp>
   <data>
      <!--
         ====================
         KEMAS EVENT STAGE
         ====================
      -->
      <record id="kemas_stage_new" model="kemas.task.type">
         <field name="sequence">1</field>
         <field name="name">New</field>
         <field name="state">draft</field>
         <field name="case_default" eval="False" />
      </record>
      <record id="kemas_stage_on_going" model="kemas.task.type">
         <field name="sequence">2</field>
         <field name="name">On going</field>
         <field name="state">pending</field>
         <field name="case_default" eval="True" />
      </record>
      <record id="kemas_stage_completed" model="kemas.task.type">
         <field name="sequence">3</field>
         <field name="name">Completed</field>
         <field name="state">done</field>
         <field name="case_default" eval="True" />
      </record>
      <record id="kemas_stage_closed" model="kemas.task.type">
         <field name="sequence">4</field>
         <field name="name">Closed</field>
         <field name="state">done</field>
         <field name="case_default" eval="True" />
         <field name="fold" eval="True" />
      </record>
      <record id="kemas_stage_canceled" model="kemas.task.type">
         <field name="sequence">5</field>
         <field name="name">Canceled</field>
         <field name="state">cancelled</field>
         <field name="case_default" eval="True" />
         <field name="fold" eval="True" />
      </record>
   </data>
</openerp>
